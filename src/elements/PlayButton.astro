---
import CircleButton, {
  type Props as CircleButtonProps,
} from './CircleButton.astro'

export interface Props extends CircleButtonProps {
  class?: string
  target?: string
}

const { class: className, ...rest } = Astro.props
---

<CircleButton
  {...rest}
  icon={'play'}
  class={`play-button ${className || ''}`}
/>
